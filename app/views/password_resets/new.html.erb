<div class="password_reset">
	<div class="container">
		<div class="form">

			<h1>Forgot Password?</h1>

			  <%= form_for (:password_reset), url: { action: "create" } do |f| %> 
			  	<%= f.email_field :email, :placeholder => "Email" %> 
			  	<%= f.submit "Reset Password", class: "btn-submit" %>
			  <% end %>
		</div>
	</div>
</div>